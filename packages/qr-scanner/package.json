{
  "name": "@aries-components/qr-scanner",
  "description": "Qr scanner specifically designed for Aries",
  "homepage": "https://github.com/animo/mobile-components",
  "repository": {
    "url": "https:://github.com/animo/mobile-components",
    "type": "git",
    "directory": "packages/qr-scanner"
  },
  "version": "0.0.0",
  "license": "Apache-2.0",
  "main": "build/index",
  "types": "build/index",
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "check-types": "tsc --noemit -p tsconfig.build.json",
    "check-format": "yarn prettier --check",
    "prettier": "prettier --ignore-path .gitignore '**/*.+(js|json|ts|md|yml|yaml)'",
    "format": "yarn prettier --write",
    "build": "yarn clean && yarn compile",
    "clean": "rimraf -rf ./lib",
    "compile": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "react-native-barcode-mask": "1.2.4",
    "expo-barcode-scanner": "11.3.0",
    "expo-camera": "12.2.0"
  },
  "peerDependencies": {
    "react-native": ">= 0.64.0",
    "react": ">= 0.16.0"
  }
}
